// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ShortenedUrl {
  id        Int      @id @default(autoincrement())
  url       String   @db.Text
  alias     String   @unique
  createdAt DateTime @default(now())
}

model User {
  id        String   @id @unique
  name      String
  email     String   @unique
  createdAt DateTime @default(now())
  imageUrl  String?
}
